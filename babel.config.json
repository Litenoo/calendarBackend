{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-transform-import-ignore", {"patterns": [".js"]}],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@src": "./src"
        }
      }
    ]
  ]
}
